{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "camelcase": "error",
    "indent": "error",
    "space-before-function-paren": "error",
    "space-before-blocks": "error",
    "semi": "error",
    "no-console": "warn",
    "no-trailing-spaces": "error"
  }
}
